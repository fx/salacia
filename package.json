{
  "name": "salacia",
  "type": "module",
  "version": "0.0.1",
  "description": "LLM inference API proxy with database recording capabilities",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "type-check": "astro check",
    "lint": "eslint src --ext .ts,.astro",
    "lint:fix": "eslint src --ext .ts,.astro --fix",
    "format": "prettier --write src/**/*.{ts,astro,json}",
    "test": "vitest",
    "test:run": "vitest run",
    "prepare": "husky"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.0",
    "@ai-sdk/openai": "^2.0.0",
    "@astrojs/react": "^4.3.0",
    "@tanstack/react-table": "^8.21.3",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@webtui/css": "^0.1.4",
    "@webtui/plugin-nf": "^0.1.1",
    "@webtui/theme-catppuccin": "^0.0.3",
    "ai": "^5.0.0",
    "astro": "^5.12.8",
    "drizzle-orm": "^0.44.4",
    "pg-hstore": "^2.3.4",
    "postgres": "^3.4.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sequelize": "^6.37.7",
    "zod": "^3.24.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.astro": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": "prettier --write"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/node": "^9.3.3",
    "@types/node": "^22.10.5",
    "@types/sequelize": "^4.28.20",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.18.0",
    "eslint-plugin-astro": "^1.2.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.4",
    "msw": "^2.8.2",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "sequelize-cli": "^6.6.3",
    "typescript": "^5.8.3",
    "vitest": "^2.1.9"
  }
}
